# Example environment for local development
# Copy to .env and fill in real values. Do not commit the real .env file.

# Use Render External Database URL; must include sslmode=require
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DBNAME?sslmode=require

# Django secret key (use a long random string)
SECRET_KEY=replace-with-a-secure-random-string

# Optional: explicitly control DEBUG for local tests
DEBUG=true
